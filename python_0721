a = [10,20,30]
a.extend([100,200])
print(a)
a.append([100,200])
print(a)
a.insert(2,[100,200])
print(a)
del a[2]
print(a)
a[1:1] = [50,60]
print(a)

a = [10,20,30]
a[2:2]=['a','b']
print(a)
# a.pop()
print(a.pop())
print(a)
# 변수카운트하고 지우는것이 오류를 안이르키는 중요해법
a.remove(10)
print(a)
print(a.index(20))
a = [10,20,30,40,50,20,40,30,30,30]
print(a.count(10))
print(a.count(20))
print(a.count(30))
print(a)
a.reverse()
print(a)
a.sort()
print(a)
a.sort(reverse=True)
print(a)
a.sort(reverse=False)
print(a)
a.clear()
print(a)
# a = [120,123,12314]

if len(a):
