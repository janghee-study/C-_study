else :
    print("값이 없습니다.")

#얇은복사 깊은복사
a = [0,0,0,0,0]
b=a
a[1]=10
print(a)
print(b)
#결과적으로는 a만 바꾸어도 b도 같이 바뀌게된다.

b=a.copy()
a[1]=30
print(a)
print(b)
#이렇게 바꾸어야 객체를 존중해준다

a=[38,32,53,62,19]
for i in a:
    print(i)
#인덱스까지 같이출력
for i,v in enumerate(a):
    print(i,v)
    

a = [32,34,543,4,123]    
smallM = 987654321
bigM = 0

while i<len(a):
    if smallM > a[i]:
        smallM=a[i]
        
    if bigM < a[i]:
        bigM=a[i]
    i+=1
print(smallM)
print(bigM)
print(min(a))
print(max(a))

a = [i for i in range(10)]
print(a)

a = list(range(0,10))
print(a)




